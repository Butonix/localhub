{
  "private": true,
  "scripts": {
    "dev": "run-p watch:js watch:css",
    "build": "run-p build:css build:js",
    "watch:js": "parcel watch ./assets/js/app.js --out-dir ./assets/dist",
    "watch:css": "postcss ./assets/css/app.css -o ./assets/dist/app.css -w",
    "build:js": "NODE_ENV=production parcel build ./assets/js/app.js --out-dir ./assets/dist",
    "build:css": "NODE_ENV=production postcss ./assets/css/app.css -o ./assets/dist/app.css"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss": "^8.1.1",
    "postcss-cli": "^8.0.0",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.4"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "date-fns": "^2.11.1",
    "hoverintent": "^2.2.1",
    "js-cookie": "^2.2.1",
    "leaflet": "^1.6.0",
    "stimulus": "^1.1.1",
    "textarea-caret": "^3.1.0",
    "turbolinks": "^5.2.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
